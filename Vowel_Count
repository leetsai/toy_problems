// Return the number (count) of vowels in the given string.
// We will consider a, e, i, o, and u as vowels for this Kata.

function getCount(str) {
  var vowelsCount = 0;
  const vowels = {
    a: true,
    e: true,
    i: true,
    o: true,
    u: true
  };
  
  for(var i = 0; i < str.length; i++) {
    let char = str[i];
    if(vowels[char]) {
      vowelsCount++;
    }
  }
  
  return vowelsCount;
}
