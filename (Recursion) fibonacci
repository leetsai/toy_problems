// Log the first n Fibonacci numbers

var fibonacci = function(n) {
	if (n === 1) {
		return [0];
	} else if (n < 1) {
		return null;
	} else if (n === 2) {
		return [0, 1];
	} else {
		var list = fibonacci(n - 1);
		var last = list.length - 1;
		list.push(list[last] + list[last - 1]);
		return list;
	}
};
